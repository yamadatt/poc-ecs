{
  "containerDefinitions": [
    {
      "name": "golang-helloworld",
      "image": "449671225256.dkr.ecr.ap-northeast-1.amazonaws.com/yamatest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8091,
          "hostPort": 8091,
          "protocol": "tcp",
          "name": "golang-helloworld",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [
        {
          "sourceVolume": "fargate-efs",
          "containerPath": "/mnt/efs"
        },
        {
          "sourceVolume": "fargate-efs2",
          "containerPath": "/mnt/efs"
        }
      ],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/yamada-ecs-task-definition",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "yamada-ecs-task-definition",
  "taskRoleArn": "arn:aws:iam::449671225256:role/yamada_ecs_task_role_name",
  "executionRoleArn": "arn:aws:iam::449671225256:role/ecsTasExecRole",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "name": "fargate-efs",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0ccff437422bd0516",
        "rootDirectory": "/"
      }
    }
  ],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
